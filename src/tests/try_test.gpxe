#!gpxe --no-exit
try
	for i in 2 1 0
	do
		dhcp net${i}
		kernel tftp://10.0.0.2//bz2bzImage
		#initrd tftp://10.0.0.2//initrd.bz2
		initrd tftp://10.0.0.2//bzImage.bz2
		boot
	done
catch
	echo "Boot failure"
done
echo "End of try-catch"
if 0
	try
		dhcp net0
		echo Fail
	catch
		echo Fail2
	done
fi
try
	dhcp net0
	kernel tftp://10.0.0.2//bz2bzImage root=100
	initrd tftp://10.0.0.2//initrd.bz2
	boot
catch
	echo "Some problem"
done
echo "End of file"